<?xml version="1.0" encoding="UTF-8"?>
<faces-config>
  <faces-config-extension>
    <namespace-uri>http://unplugged.teamstudio.com</namespace-uri>
    <default-prefix>unp</default-prefix>
  </faces-config-extension>
  <composite-component>
    <component-type>unpCalendar</component-type>
    <composite-name>unpCalendar</composite-name>
    <composite-file>/unpCalendar.xsp</composite-file>
    <composite-extension>
      <designer-extension>
        <in-palette>true</in-palette>
        <category>Unplugged</category>
      </designer-extension>
    </composite-extension>
    <icon>
      <small-icon>/icons/defaultLogoSmall.png</small-icon>
      <large-icon>/icons/defaultLogo.png</large-icon>
    </icon>
    <property>
      <property-name>viewname</property-name>
      <property-class>string</property-class>
    </property>
    <property>
      <property-name>startdatefield</property-name>
      <property-class>string</property-class>
    </property>
    <property>
      <property-name>enddatefield</property-name>
      <property-class>string</property-class>
    </property>
    <property>
      <property-name>titlefield</property-name>
      <property-class>string</property-class>
    </property>
    <property>
      <property-name>viewxpage</property-name>
      <property-class>string</property-class>
    </property>
    <property>
      <property-name>highlightfield</property-name>
      <property-class>string</property-class>
    </property>
    <property>
      <property-name>highlighttest</property-name>
      <property-class>string</property-class>
    </property>
    <property>
      <property-name>headerbuttonsleft</property-name>
      <property-class>string</property-class>
      <property-extension>
        <designer-extension>
          <default-value>prev,today,next</default-value>
        </designer-extension>
        <required>true</required>
      </property-extension>
    </property>
    <property>
      <property-name>headerbuttonsrighttablet</property-name>
      <property-class>string</property-class>
      <property-extension>
        <designer-extension>
          <default-value>agendaDay,agendaWeek,month</default-value>
        </designer-extension>
      </property-extension>
    </property>
    <property>
      <property-name>headerbuttonsrightphone</property-name>
      <property-class>string</property-class>
    </property>
    <property>
      <property-name>defaultviewtablet</property-name>
      <property-class>string</property-class>
      <property-extension>
        <designer-extension>
          <default-value>month</default-value>
        </designer-extension>
      </property-extension>
    </property>
    <property>
      <property-name>defaultviewphone</property-name>
      <property-class>string</property-class>
      <property-extension>
        <designer-extension>
          <default-value>basicWeek</default-value>
        </designer-extension>
      </property-extension>
    </property>
    <property>
      <property-name>filter</property-name>
      <property-class>string</property-class>
    </property>
  </composite-component>
</faces-config>
